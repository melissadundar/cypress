{
  "dependencies": {
    "cypress": "^13.6.2"
  },
  "devDependencies": {
    "cypress-xpath": "^2.0.1",
    "mochawesome": "^7.1.3"
  },
  "scripts": {
    "cy:run-firefox": "cypress run --browser=firefox",
    "cy:run-chrome": "cypress run --browser=chrome",
    "cy:run-chrome-open": "cypress open --browser=chrome",
    "report:clean": "rmdir -rf report cypress/screenshot cypress/reports cypress/videos cypress/mochawesome-report",
    "report:mochawesome": "cypress run --reporter mochawesome"
  }
}
